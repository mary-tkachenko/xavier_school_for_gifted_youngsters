<%= form_with model: @cohort, local: true do |form| %>
  <%= render "shared/errors", object: @cohort %>
  Name: <%= form.text_field :name, placeholder: "name"%>
  Auditorium: <%= form.text_field :auditorium %>
  Start Date: <%= form.text_field :start_date %>
  End Date: <%= form.text_field :end_date %>
  Srudents Total: <%= form.text_field :students_number %>
  <%= form.hidden_field :user_id, value: @cohort_id %>
  <%= form.submit %>
<% end %> 