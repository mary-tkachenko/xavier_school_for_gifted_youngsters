<%= form_with model: @instructor do |form| %>
  First Name: <%= form.text_field :first_name, placeholder: "name"%>
  Last Name: <%= form.text_field :last_name %>
  Nickname: <%= form.text_field :nickname %>
  Age: <%= form.text_field :age %>
  Abilities: <%= form.text_field :abilities %>
  Salary: <%= form.text_field :salary %>
  Photo: <%= form.text_field :photo_url %>
  Education: <%= form.text_field :education %>
  Course: <%= form.text_field :course_id %>
  <%= form.hidden_field :user_id, value: @instructor_id %>
  <%= form.submit %>
<% end %> 